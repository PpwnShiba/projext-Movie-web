<%- include('partials/header-nav.ejs') %> 

<div class="search-movie-mobile">
  <form class="d-flex " action="/movies/search-movie" method="post">
    <input class="search-cinema" type="search" name="search_location" placeholder="  Search movies Here . . ." aria-label="Search">
    <button type="submit">
      <i class="fas fa-search fa-2x"></i>
    </button>
  </form>
</div>
<div class="header-movies-page" >
  <div class="body">
    <img src="data:image/svg+xml;base64," />
    <span class="movies-text">Movies</span><br>
    <span class="nowshowing-text">Coming Soon</span> 
   </div>
 </div>
<div class="container movies">
    <!--  GRID CARD MOVIES Coming Soon -->
    <div  id="page-comingsoon">
      <div class="album py-5 ">
        <div class="container movie-cards">
          <div class="row g-3 row-cols-1 row-cols-sm-2 row-cols-md-4 ">
            <% allmovies_coming.forEach(function(allmovies_coming){ %>
            <div class="col">
              <div class="card">
                <img src="<%= allmovies_coming.image %>" class="card-img-top">
  
                  <div class="card-body">
                    <a href="/Movies/Movie-Showtime">
                      <i class="fas fa-play play-btn" data-toggle="tooltip" data-placement="bottom" title="Play Trailer" ></i>
                    </a>
                    <h6 class="card-title"><%= allmovies_coming.name %></h6>
                    <!-- <span class="movie-info"><i class="fas fa-star"></i> 9 / 10 </span> -->
                  </div> 
              
              </div>
            </div>
            <% }) %>
          </div>
        </div>
        
      
    </div>
</div>
</div>


<%- include('partials/footer.ejs') %> 