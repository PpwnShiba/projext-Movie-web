
<%- include('partials/Header-nav.ejs') %> 

<!-- Slider main container -->
<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
  <div class="swiper-container">

    <div class="swiper-wrapper">
      <!-- Slides -->
      <div class="swiper-slide">
        <img src="https://bingeddata.s3.amazonaws.com/uploads/2020/12/pixels-1.jpg" class="d-block w-100" >
      </div>
      <div class="swiper-slide">
        <img src="https://www.gamemonday.com/wp-content/uploads/2016/12/gaurdians.jpg" class="d-block w-100">
      </div>
      <div class="swiper-slide">
        <img src="https://bookitnow.pk/blog/wp-content/uploads/2017/07/Spider-Man-Homecoming-official-Wallpapers-HD-1920-x-1080-4-1.jpg" class="d-block w-100" >
      </div>
      <div class="swiper-slide">
        <img src="https://lightbox-prod.imgix.net/images/assets/100284450-6593844-The_Call_of_the_Wild_2020_CC2.jpg?auto=format&fit=crop&crop=faces%2Cright&ar=16%3A9&ixlib=react-9.0.1"  class="d-block w-100" >
      </div>
      <div class="swiper-slide">
        <img src="https://images.hdqwalls.com/wallpapers/godzilla-vs-kong-movie-poster-5k-oq.jpg"  class="d-block w-100" >
      </div>
      <div class="swiper-slide">
        <img src="https://www.irrompibles.net/irrwp/wp-content/uploads/2019/01/images_2019_cine_Spider-Man_Into_the_Spider-Verse_Portada_review_Spiderman_Spiderverse.jpg" class="d-block w-100" >
      </div>
      
    </div>
  <!-- If we need pagination -->
  <div class="swiper-pagination"></div>

  <!-- If we need navigation buttons -->
  <div class="swiper-button-prev"></div>
  <div class="swiper-button-next"></div>


</div>
</div>

<div class="search-movie-mobile">
  <form class="d-flex " action="/movies/search-movie" method="post">
    <input class="search-cinema" type="search" name="search_location" placeholder="  Search movies Here . . ." aria-label="Search">
    <button type="submit">
      <i class="fas fa-search fa-2x"></i>
    </button>
  </form>
</div>

 <!--  Header MOVIES  -->
<div class="header">
  <input type="radio" class="btn-check " id="option1" autocomplete="off" checked  onclick="shownow()" >
  <label class="btn btn-now " id="btn-now" style="background-color: #333;" for="option1">Movies <b> Now Showing </b><img class="icon-btn" src="data:image/svg+xml;base64," /></label>
  <input type="radio" class="btn-check " id="option2" autocomplete="off"  onclick="showsoon()" >
  <label class="btn btn-now " id="btn-soon" for="option2">Movies <b> Coming Soon </b><img class="icon-btn" src="data:image/svg+xml;base64," /></button>
</div>



 <!--  Alert  -->


<div class="container movies">
    <!--  GRID CARD MOVIES Now showing -->
    
  <div id="page-nowshowing">
    <div class="album py-5 ">
      <div class="container movie-cards mt-5 ">
        <div class="row g-3 justify-content-center">
          <% var i = 0; %>
          <% allmovies_now.forEach(function(allmovies_now){ %>
          <div class="col ">
            <div class="card">
              <img src="<%= allmovies_now.image %>" class="card-img-top">

                <div class="card-body">
                  <a href="/Movies/Movie-Showtime/<%= allmovies_now._id %>">
                    <i class="fas fa-play play-btn" data-toggle="tooltip" data-placement="bottom" title="Play Trailer" ></i>
                  </a>
                  
                  <h6 class="card-title"><%= allmovies_now.name %></h6>
                  
                  <form id="btn-form" action="/user/myfav/<%= allmovies_now._id %>/<%= allmovies_now.name %>" method="GET" >
                    <button type="button" class="btn btn-danger btn-addfav" data-bs-toggle="modal" data-bs-target="#Modal<%= i %>">
                      <i class="fas fa-heart fa-lg"></i><span>Add to my favorite</span>
                    </button>
                    <div class="modal fade modal-addfav" id="Modal<%= i %>" tabindex="-1">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                          </div>
                          <div class="modal-body" ><i class="far fa-laugh fa-6x" style="margin-left: 120px; margin-bottom: 20px;" ></i>
                            <p style="text-align: center;">Add <%= allmovies_now.name %> to favorite?</p> 
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>                         
                            <button type="submit" class="btn btn-success">Confirm</button>
                          </div>
                        </div>
                      </div>
                    </div>
                    </form>
                  
                  <span class="movie-info"><i class="fas fa-star" style="margin-right:  2px;"></i><%= allmovies_now.score %> / 10</span>
                </div> 
            
            </div>
          </div>
          <% i++;}) %>
        </div>
      </div>
    </div>
  </div>
  <div  id="page-comingsoon" style="display: none;">
    <div class="album py-5 ">
      <div class="container movie-cards">
        <div class="row g-3 justify-content-center ">
          <% var i = 0; %>
          <% allmovies_comingsoon.forEach(function(allmovies_comingsoon){ %>
          <div class="col">
            <div class="card">
              <img src="<%= allmovies_comingsoon.image %>" class="card-img-top">

                <div class="card-body">
                  <a href="/Movies/movie-details/comingsoon/<%= allmovies_comingsoon._id %>">
                    <i class="fas fa-play play-btn" data-toggle="tooltip" data-placement="bottom" title="Play Trailer" ></i>
                  </a>
                  <h6 class="card-title"><%= allmovies_comingsoon.name %></h6>
                  
                  <form id="btn-form" action="/user/myfav/comingsoon/<%= allmovies_comingsoon._id %>/<%= allmovies_comingsoon.name %>" method="GET" >
                    <button type="button" class="btn btn-danger btn-addfav" data-bs-toggle="modal" data-bs-target="#Modal_coming<%= i %>">
                      <i class="fas fa-heart fa-lg"></i><span>Add to my favorite</span>
                    </button>
                    <div class="modal fade modal-addfav" id="Modal_coming<%= i %>" tabindex="-1">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                          </div>
                          <div class="modal-body" ><i class="far fa-laugh fa-6x" style="margin-left: 120px; margin-bottom: 20px;" ></i>
                            <p style="text-align: center;">Add <%= allmovies_comingsoon.name %> to favorite?</p> 
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>                         
                            <button type="submit" class="btn btn-success">Confirm</button>
                          </div>
                        </div>
                      </div>
                    </div>
                  </form>
                </div> 
            
            </div>
          </div>
          <%i++ }); %>
        </div>
      </div>
      
    
  </div>
</div>

<script>
  const swiper = new Swiper('.swiper-container', {
  // Optional parameters
  direction: 'horizontal',
  loop: true,

  // If we need pagination
  pagination: {
    el: '.swiper-pagination',
  },

  // Navigation arrows
  navigation: {
    nextEl: '.swiper-button-next',
    prevEl: '.swiper-button-prev',
  },

});

function shownow(){

    var nowshowing = document.getElementById('page-nowshowing').style.display='block';
    var comingsoon = document.getElementById('page-comingsoon').style.display='none';
    document.getElementById('btn-now').style.backgroundColor='#333';
    document.getElementById('btn-now').style.Color='#fff';
    document.getElementById('btn-soon').style.Color='#333';
    document.getElementById('btn-soon').style.backgroundColor='#fff';
}
function showsoon(){
    var nowshowing = document.getElementById('page-nowshowing').style.display='none';
    var comingsoon = document.getElementById('page-comingsoon').style.display='block';
    document.getElementById('btn-now').style.backgroundColor='#fff';
    document.getElementById('btn-now').style.Color='#333';
    document.getElementById('btn-soon').style.backgroundColor='#333';
    document.getElementById('btn-soon').style.Color='#fff';
}
</script>
  
<%- include('partials/footer.ejs') %> 