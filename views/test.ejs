<div class="container">
    <h1 class="text-center">Multer Multiple Field File Upload Example</h1>
    <form action="/admin/add-movies" id="post" method="post" enctype="multipart/form-data">
      <div class="form-group">
        <label for="file1">Upload File 1:</label>
        <input type="file" name="poster" id="" required class="form-control">
      </div>
      <div class="form-group">
          <label for="file2">Upload File 2:</label>
          <input type="file" name="header" id="" required class="form-control">
        </div>
        <div class="form-group">
            <button class="btn btn-danger btn-block">
                Upload Files
            </button>
        </div>
  </form>
</div>

